{
  "name": "my-automated-blog",
  "version": "1.0.0",
  "description": "My awesome automated Eleventy blog",
  "main": ".eleventy.js",
  "scripts": {
    "build": "npx @11ty/eleventy && npm run generate:og",
    "start": "npx @11ty/eleventy --serve",
    "generate:og": "node scripts/generate-og.js",
    "validate:frontmatter": "node scripts/validateFrontmatter.js",
    "lint": "echo \"No linting configured\"",
    "test": "npm run validate:frontmatter"
  },
  "keywords": [
    "eleventy",
    "blog",
    "static-site-generator"
  ],
  "author": "Your Name Here",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-plugin-rss": "^2.0.3",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "gray-matter": "^4.0.3",
    "luxon": "^3.4.4",
    "nunjucks": "^3.2.4",
    "puppeteer": "^22.7.1",
    "slugify": "^1.6.6"
  }
}
