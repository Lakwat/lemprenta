{
  "name": "my-automated-blog",
  "version": "1.0.0",
  "description": "My awesome automated Eleventy blog",
  "main": ".eleventy.js",
  "scripts": {
    "build": "npx @11ty/eleventy",
    "start": "npx @11ty/eleventy --serve",
    "lint:css": "npx stylelint \"assets/css/**/*.css\"",
    "lint:html": "npx htmlhint \"_site/**/*.html\"",
    "lint": "npm run lint:css && npm run lint:html",
    "test": "npm run lint"
  },
  "keywords": [
    "eleventy",
    "blog",
    "static-site-generator"
  ],
  "author": "Your Name Here",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "htmlhint": "^1.1.4",
    "luxon": "^3.4.4",
    "slugify": "^1.6.6",
    "stylelint": "^16.19.1",
    "stylelint-config-standard": "^38.0.0"
  }
}
